	header "/usr/include/GL/glx.h"
  }
  module Glu {
	header "/usr/include/GL/glu.h"
  }
  module X {
      header "/usr/include/X11/X.h"
  }
  module X {
      header "/usr/include/X11/X.h"
  }
  module Xatom {
      header "/usr/include/X11/Xatom.h"
  }
